{
  "name": "nymvpn-x",
  "private": true,
  "version": "0.1.1-dev",
  "type": "module",
  "authors": [
    "pierre <dommerc.pierre@gmail.com>",
    "zane <zanecschepke@gmail.com>",
    "nym"
  ],
  "description": "Nym VPN Desktop Client",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "dev:app": "tauri dev",
    "dev:browser": "vite --mode dev-browser",
    "check": "run-s lint tscheck fmt",
    "build": "tsc && vite build",
    "build:app": "npm run tauri build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts --fix src/",
    "fmt": "prettier --write --ignore-unknown \"**/*\"",
    "fmt:check": "prettier --check --ignore-unknown \"**/*\"",
    "tscheck": "tsc --noEmit",
    "tauri": "tauri",
    "gen:licenses": "run-p --aggregate-output gen:licenses:rust gen:licenses:js",
    "gen:licenses:js": "npx license-checker-rseidelsohn --json --customPath license-fmt.json > public/licenses-js.json",
    "gen:licenses:rust": "cd src-tauri && cargo about generate --all-features --format json -o ../public/licenses-rust.json"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@headlessui/tailwindcss": "^0.2.0",
    "@mui/base": "^5.0.0-beta.39",
    "@sentry/integrations": "^7.106.1",
    "@sentry/react": "^7.106.1",
    "@tauri-apps/api": "^1.5.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.13",
    "i18next": "^23.7.6",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.18.0",
    "react-virtualized-auto-sizer": "^1.0.23",
    "react-window": "^1.8.10"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.0",
    "@types/lodash-es": "^4.17.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-import": "^16.0.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.5",
    "vite-plugin-svgr": "^4.2.0"
  }
}
