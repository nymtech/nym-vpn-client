{
  "name": "nym-vpn-app",
  "private": true,
  "version": "0.2.1-dev",
  "type": "module",
  "authors": [
    "pierre <dommerc.pierre@gmail.com>",
    "zane <zanecschepke@gmail.com>",
    "nym"
  ],
  "description": "Nym VPN Desktop Client",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "dev:app": "tauri dev",
    "dev:browser": "vite --mode dev-browser",
    "check": "run-s lint tscheck fmt",
    "build": "tsc && vite build",
    "build:app": "npm run tauri build",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "fmt": "prettier --write --ignore-unknown \"**/*\"",
    "fmt:check": "prettier --check --ignore-unknown \"**/*\"",
    "tscheck": "tsc --noEmit",
    "tauri": "tauri",
    "gen:licenses": "run-p --aggregate-output gen:licenses:rust gen:licenses:js",
    "gen:licenses:js": "npx --yes license-checker-rseidelsohn@4.3.0 --production --json > public/licenses-js.json",
    "gen:licenses:rust": "cargo license -j --avoid-dev-deps --avoid-build-deps --manifest-path src-tauri/Cargo.toml > public/licenses-rust.json"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@mui/base": "^5.0.0-beta.39",
    "@sentry/react": "^8.22.0",
    "@tauri-apps/api": ">=2.0.0-rc.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.0.0-rc.1",
    "@tauri-apps/plugin-dialog": "^2.0.0-rc",
    "@tauri-apps/plugin-notification": "^2.0.0-rc",
    "@tauri-apps/plugin-os": "^2.0.0-rc",
    "@tauri-apps/plugin-process": "^2.0.0-rc",
    "@tauri-apps/plugin-shell": "^2.0.0-rc",
    "@tauri-apps/plugin-updater": "^2.0.0-rc",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.5.6",
    "i18next": "^23.12.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.0",
    "react-router-dom": "^6.24.1",
    "react-virtualized-auto-sizer": "^1.0.23",
    "react-window": "^1.8.10"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@tauri-apps/cli": ">=2.0.0-rc.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^5.1.0-rc-187dd6a7-20240806",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "postcss-import": "^16.0.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.5",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.7",
    "vite-plugin-svgr": "^4.2.0"
  }
}
